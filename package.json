	"name": "filen-mobile",
	"main": "./index",
	"version": "0.0.0",
	"private": false,
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"enabled": false
			}
		}
	},
	"scripts": {
		"start": "expo start --clear",
		"android": "expo run:android",
		"android:device": "expo run:android --device",
		"android:release": "expo run:android --variant release",
		"android:release:device": "expo run:android --variant release --device",
		"android:eas:dev": "eas build --platform android --profile development",
		"android:eas:release": "eas build --platform android --profile production",
		"ios": "expo run:ios",
		"ios:release": "expo run:ios --configuration Release",
		"ios:eas:dev": "eas build --platform ios --profile development",
		"ios:eas:release": "eas build --platform ios --profile production",
		"ios:device": "expo run:ios --device",
		"ios:device:release": "expo run:ios --device --configuration Release",
		"web": "expo start --web",
		"test": "jest --watchAll",
		"lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
		"postinstall": "patch-package",
		"prebuild": "npm run install:prebuilds && expo prebuild",
		"prebuild:clean": "npm run clean && npm run install:prebuilds && expo prebuild --clean",
		"prebuild:ci:android": "npm run superclean && npm run install:prebuilds && expo prebuild --clean --platform android",
		"prebuild:ci:ios": "npm run superclean && npm run install:prebuilds && expo prebuild --clean --platform ios",
		"tsc:check": "tsc -p ./tsconfig.json",
		"install:filen": "npm install @filen/sdk@latest",
		"ncu": "ncu --interactive --format group",
		"install:prebuilds": "tsx ./installPrebuilds.ts",
		"clean": "tsx ./clean.ts",
		"superclean": "tsx ./superclean.ts",
		"buildNodeThread": "node buildNodeThread.js"
	},
	"jest": {
		"preset": "jest-expo"
	},
