{
	"name": "filen-mobile",
	"version": "2.0.70",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint .",
		"clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
		"dist-android-clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && cd ./android && ./gradlew clean --parallel --max-workers=4 && ./gradlew bundleRelease --parallel --max-workers=4 && ./gradlew assembleRelease --parallel --max-workers=4 && cd ..",
		"dist-android": "cd ./android && ./gradlew bundleRelease --parallel --max-workers=4 && ./gradlew assembleRelease --parallel --max-workers=4 && cd ..",
		"dist-android-apk": "cd ./android && ./gradlew assembleRelease --parallel --max-workers=4 && cd ..",
		"splash": "react-native generate-bootsplash bootsplash.png --background-color=#263179 --logo-width=100 --flavor=main --assets-path=src/assets",
		"icons": "react-native-svg-app-icon",
		"install-release-apk-op": "adb -s 6c7a7e7b install android/app/build/outputs/apk/release/app-release.apk",
		"install-release-apk-op2": "adb -s 1c6c9bc3 install android/app/build/outputs/apk/release/app-release.apk",
		"install-release-apk-sm": "adb -s RZCW20AMCBB install android/app/build/outputs/apk/release/app-release.apk",
		"install-release-apk-emulator": "adb -s emulator-5554 install android/app/build/outputs/apk/release/app-release.apk",
		"android-release": "react-native run-android --variant=release --deviceId=RZCW20AMCBB",
		"check-dependencies": "rnx-align-deps",
		"fix-dependencies": "rnx-align-deps --write",
		"postinstall": "patch-package",
		"tsc:check": "tsc --noEmit"
	},
	"dependencies": {
		"@filen/filen-sdk": "^1.0.8",
		"@notifee/react-native": "^7.8.0",
		"@openspacelabs/react-native-zoomable-view": "^2.1.5",
		"@react-native-community/hooks": "^3.0.0",
		"@react-native-community/netinfo": "^11.2.1",
		"@react-native-picker/picker": "^2.6.1",
		"@react-navigation/native": "^6.1.9",
		"@react-navigation/native-stack": "^6.9.17",
		"@sentry/react-native": "^5.16.0",
		"@shopify/flash-list": "^1.6.3",
		"axios": "^1.6.5",
		"crypto-js": "4.1.1",
		"emoji-js": "^3.8.0",
		"emojibase-regex": "^15.3.0",
		"expo": "^48.0.6",
		"expo-background-fetch": "^11.8.1",
		"expo-clipboard": "^4.1.2",
		"expo-file-system": "^15.2.2",
		"expo-font": "^11.1.1",
		"expo-keep-awake": "^12.0.1",
		"expo-local-authentication": "^13.2.1",
		"expo-media-library": "^15.2.3",
		"expo-navigation-bar": "^2.1.1",
		"expo-network": "^5.2.1",
		"expo-status-bar": "^1.4.4",
		"expo-task-manager": "^11.7.2",
		"expo-video-thumbnails": "^7.2.1",
		"graceful-fs": "4.2.9",
		"intl": "1.2.5",
		"lodash": "4.17.21",
		"mime-types": "^2.1.35",
		"nodejs-mobile-react-native": "github:Dwynr/nodejs-mobile-react-native",
		"react": "18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.71.13",
		"react-native-actions-sheet": "^0.9.0-alpha.24",
		"react-native-blob-util": "^0.19.6",
		"react-native-bootsplash": "^5.4.0",
		"react-native-cn-quill": "^0.7.18",
		"react-native-device-info": "^10.12.0",
		"react-native-dialog": "^9.3.0",
		"react-native-document-picker": "^9.1.0",
		"react-native-fast-image": "github:nReality/react-native-fast-image",
		"react-native-file-viewer": "^2.1.5",
		"react-native-gesture-handler": "^2.14.1",
		"react-native-heic-converter": "^1.3.2",
		"react-native-html-parser": "^0.1.0",
		"react-native-image-picker": "^7.1.0",
		"react-native-image-resizer": "^1.4.5",
		"react-native-ios-asset-exporter": "^0.1.7",
		"react-native-marked": "^6.0.4",
		"react-native-mmkv": "^2.10.2",
		"react-native-notifications": "^5.1.0",
		"react-native-permissions": "^4.0.4",
		"react-native-picker-select": "^9.0.0",
		"react-native-progress": "^5.0.1",
		"react-native-reanimated": "^3.6.1",
		"react-native-saf-x": "^2.2.3",
		"react-native-safe-area-context": "^4.8.2",
		"react-native-scoped-storage": "^1.9.5",
		"react-native-screens": "^3.29.0",
		"react-native-share": "^10.0.2",
		"react-native-share-menu": "^6.0.0",
		"react-native-sqlite-storage": "^6.0.1",
		"react-native-svg": "^14.1.0",
		"react-native-toast-notifications": "^3.4.0",
		"react-native-webview": "^13.6.3",
		"react-native-windows": "^0.71.0",
		"react-native-zoom-reanimated": "^1.4.1",
		"react-use": "^17.4.0",
		"regexify-string": "^1.0.19",
		"socket.io-client": "^2.5.0",
		"striptags": "3.2.0",
		"uuid": "9.0.0",
		"zustand": "^4.0.0"
	},
	"overrides": {
		"react-native-picker-select": {
			"@react-native-picker/picker": "$@react-native-picker/picker"
		}
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "7.1.6",
		"@babel/runtime": "7.12.5",
		"@react-native-community/eslint-config": "2.0.0",
		"@rnx-kit/align-deps": "^2.2.0",
		"@types/emoji-js": "^3.5.2",
		"@types/jest": "28.1.6",
		"@types/lodash": "4.14.182",
		"@types/mime-types": "2.1.1",
		"@types/node": "^18.15.11",
		"@types/react": "~18.0.27",
		"@types/react-dom": "~18.0.10",
		"@types/react-native": "^0.71.3",
		"@types/react-native-background-timer": "2.0.0",
		"@types/react-native-share-menu": "5.0.2",
		"@types/react-native-sqlite-storage": "6.0.0",
		"@types/react-test-renderer": "18.0.0",
		"@types/uuid": "^9.0.1",
		"babel-jest": "26.6.3",
		"babel-plugin-transform-remove-console": "6.9.4",
		"eslint": "7.14.0",
		"jest": "^29.2.1",
		"metro-react-native-babel-preset": "^0.73.7",
		"path": "0.12.7",
		"prettier": "2.8.4",
		"prettier-plugin-x": "0.0.10",
		"react-native-svg-app-icon": "0.6.1",
		"react-test-renderer": "18.2.0",
		"typescript": "^4.9.4"
	},
	"jest": {
		"preset": "react-native"
	},
	"rnx-kit": {
		"kitType": "app",
		"alignDeps": {
			"requirements": [
				"react-native@0.71"
			],
			"capabilities": [
				"babel-preset-react-native",
				"core",
				"core-android",
				"core-ios",
				"core-windows",
				"filesystem",
				"gestures",
				"hooks",
				"jest",
				"navigation/native",
				"navigation/stack",
				"netinfo",
				"react",
				"react-dom",
				"react-test-renderer",
				"safe-area",
				"screens",
				"sqlite",
				"svg"
			]
		}
	},
	"reactNativePermissionsIOS": [
		"Camera",
		"FaceID",
		"MediaLibrary",
		"Microphone",
		"PhotoLibrary",
		"PhotoLibraryAddOnly"
	]
}
