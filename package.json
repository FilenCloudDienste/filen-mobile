{
  "name": "filen-mobile",
  "version": "2.0.32",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "dist-android": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && cd ./android && ./gradlew bundleRelease && ./gradlew assembleRelease && cd ..",
    "splash": "react-native generate-bootsplash bootsplash.png --background-color=#263179 --logo-width=100 --flavor=main --assets-path=src/assets",
    "icons": "react-native-svg-app-icon",
    "install-release-apk": "adb -s 6c7a7e7b install android/app/build/outputs/apk/release/app-release.apk",
    "install-release-apk-emulator": "adb -s emulator-5554 install android/app/build/outputs/apk/release/app-release.apk"
  },
